{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "*": [
          "./*"
        ],
        "pages/*": [
            "./pages/*"
        ]
      },
      "target": "es2017",
      "module": "commonjs",
      "strict": true,
      "noImplicitAny": false,
      "typeRoots": ["./types"],
      "types": ["@wdio/sync", "node", "mocha", "chai", "@wdio/mocha-framework", ],
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    }
  }